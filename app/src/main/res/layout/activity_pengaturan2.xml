<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pengaturan">

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="413dp"
        android:layout_height="284dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/blokkuning" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="82dp"
        android:layout_height="78dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:backgroundTint="@color/yellow"
        android:onClick="Dashboard"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/logoback" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="181dp"
        android:layout_height="177dp"
        android:layout_marginTop="76dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView25"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pengaturan1" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pengaturan"
        app:layout_constraintBottom_toTopOf="@+id/imageView26"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView25" />

    <Button
        android:id="@+id/button3"
        android:layout_width="326dp"
        android:layout_height="66dp"
        android:layout_marginTop="36dp"
        android:text="Auto-Text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView25"
        android:textAlignment="viewStart"/>

    <Button
        android:id="@+id/button6"
        android:layout_width="326dp"
        android:layout_height="66dp"
        android:layout_marginTop="16dp"
        android:text="Aktivasi Keyboard"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8"
        android:textAlignment="viewStart"/>

    <Button
        android:id="@+id/button8"
        android:layout_width="326dp"
        android:layout_height="66dp"
        android:layout_marginTop="12dp"
        android:text="Pengingat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        android:textAlignment="viewStart"/>

    <Button
        android:id="@+id/button7"
        android:layout_width="326dp"
        android:layout_height="66dp"
        android:layout_marginTop="12dp"
        android:text="Invoice"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        android:textAlignment="viewStart"/>

</androidx.constraintlayout.widget.ConstraintLayout>